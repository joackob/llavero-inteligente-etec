{
  "name": "next-auth-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./db/init.mjs && next dev",
    "start": "node ./db/init.mjs && next start",
    "build": "next build",
    "tests:e2e": "next build && bunx playwright test",
    "tests:e2e:without-build": "bunx playwright test",
    "tests": "bun test **/*.test.ts",
    "tests:watch": "bun test --watch **/*.test.ts",
    "create:test:e2e": "bunx playwright codegen",
    "lint": "next lint",
    "generate:db": "bunx prisma generate",
    "migrate:dev:db": "bunx prisma migrate dev --name dev",
    "migrate:deploy:db": "bunx prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^10.14.0",
    "jose": "^5.8.0",
    "lucide-react": "^0.441.0",
    "mqtt": "^5.10.1",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.47",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.11",
    "typescript": "^5"
  },
  "module": "index.ts",
  "type": "module"
}
